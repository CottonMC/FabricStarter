{
  "schema_version": 1,
  "quilt_loader": {
    "group": "io.github.cottonmc",
    "id": "starter",
    "version": "${version}",
    "metadata": {
      "name": "Fabric Starter",
      "icon": "assets/starter/icon.png",
      "description": "Template for starting out Fabric mods",
      "license": "MIT",
      "contact": {
        "sources": "https://github.com/CottonMC/FabricStarter"
      }
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "io.github.cottonmc.starter.FabricStarter"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.0-"
      }
    ]
  },
  "mixin": "mixins.starter.json"
}
